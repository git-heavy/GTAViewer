<Application x:Class="Heavy.DFFViewer.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:vm="clr-namespace:Heavy.DFFViewer.ViewModel"
             xmlns:v="clr-namespace:Heavy.DFFViewer.View"
             Startup="Application_Startup">
  <Application.Resources>
    <DataTemplate DataType="{x:Type vm:MainWindowViewModel}">
      <v:MainWindowView/>
    </DataTemplate>
    <!--<DataTemplate DataType="{x:Type vm:GeometryObjectViewModel}">
      <v:GeometryObjectView/>
    </DataTemplate>-->
  </Application.Resources>
</Application>
